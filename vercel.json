{
    "version": 2,
    "name": "eloin-fitness-academia",
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/login",
            "dest": "public/login.html"
        },
        {
            "src": "/painel",
            "dest": "public/painel.html"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
            "dest": "public/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    }
}