{
    "version": 2,
    "name": "eloin-fitness-academia",
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/login",
            "destination": "/public/login.html"
        },
        {
            "source": "/painel",
            "destination": "/public/painel.html"
        },
        {
            "source": "/",
            "destination": "/public/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}